      <xsd:extension base="esdl:Storage">
        <xsd:attribute ecore:unsettable="false" name="selfDischarge" type="ecore:EDouble"/>
        <xsd:attribute ecore:unsettable="false" name="maxChargeDischargeCycli" type="ecore:EInt"/>
        <xsd:attribute default="0.0" ecore:unsettable="false" name="chargeEfficiency" type="ecore:EDouble"/>
        <xsd:attribute default="0.0" ecore:unsettable="false" name="dischargeEfficiency" type="ecore:EDouble"/>
        <xsd:attribute ecore:unsettable="false" name="maxChargeRate" type="ecore:EDouble"/>
        <xsd:attribute ecore:unsettable="false" name="maxDischargeRate" type="ecore:EDouble"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
