    <xsd:sequence>
      <xsd:element ecore:resolveProxies="true" minOccurs="0" name="location" type="esdl:Point"/>
      <xsd:element ecore:resolveProxies="true" minOccurs="0" name="contour" type="esdl:Polygon"/>
      <xsd:element ecore:resolveProxies="true" minOccurs="0" name="socialProperties" type="esdl:SocialProperties"/>
