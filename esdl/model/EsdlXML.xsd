          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="flatRoofArea" type="xsd:double">
          <xsd:annotation>
            <xsd:documentation>in m2</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="roofType" type="esdl:RoofTypeEnum">
