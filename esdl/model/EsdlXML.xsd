  <xsd:complexType name="OutPort">
    <xsd:annotation>
      <xsd:documentation>Represents a port with a positive energy direction out of the asset, e.g. for a Producer. See Port for more details</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
