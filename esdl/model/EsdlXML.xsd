    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="esdl:Port">
        <xsd:attribute name="connectedTo" type="xsd:string"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="InPort" type="esdl:InPort">
