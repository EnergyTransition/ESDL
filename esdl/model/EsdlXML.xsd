          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="COP" type="xsd:double">
          <xsd:annotation>
            <xsd:documentation>Coefficient of Performance of this source</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="aquiferTemperature" type="xsd:double">
