        <xsd:attribute name="pumpPower" type="xsd:double">
          <xsd:annotation>
            <xsd:documentation>in Watt</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="geothermalPotential" type="xsd:string">
          <xsd:annotation>
            <xsd:documentation>references the potential this source is part of</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
