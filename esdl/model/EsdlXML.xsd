    <xsd:attribute name="aggrType" type="esdl:AggrTypeEnum">
      <xsd:annotation>
        <xsd:documentation>(experimental) defines how the data in this instance is aggregated, e.g. by commodity, total energy, per capability, etc.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
