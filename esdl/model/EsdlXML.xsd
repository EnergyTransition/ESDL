        <xsd:attribute name="height" type="xsd:double">
          <xsd:annotation>
            <xsd:documentation>in m</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="rcFloor" type="xsd:double">
          <xsd:annotation>
            <xsd:documentation>in m2K/W</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="rcWall" type="xsd:double">
          <xsd:annotation>
            <xsd:documentation>in m2K/W</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="rcRoof" type="xsd:double">
          <xsd:annotation>
            <xsd:documentation>in m2K/W</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="uWindow" type="xsd:double">
